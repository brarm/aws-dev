# login to aws ecr
$(aws ecr get-login --no-include-email --region us-west-1)

# Build docker image from dockerfile
docker build --no-cache -t wb-app/docker-images .

# Tag with URL of ECR
docker tag wb-app/docker-images:latest 799480191532.dkr.ecr.us-west-1.amazonaws.com/wb-app/docker-images:latest

# Push container to ECR
docker push 799480191532.dkr.ecr.us-west-1.amazonaws.com/wb-app/docker-images:latest
